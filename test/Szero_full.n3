:description a tmt:Description ;
   tmt:hasSubDescription :descriptionMales ;  ### or we could allow recursive Description
   tmt:hasSubDescription :descriptionFemales .

:descriptionMales a tmt:Description;
        tmt:hasScope tmt:males ;
	    tmt:trait :trait1 ;  #colours
	    tmt:trait :trait2 ;  #total length with cephalic lobe
        tmt:trait :trait3 .  #total length without cephalic lobe

:descriptionFemales a tmt:Description ;
        tmt:hasScope tmt:females ;
        tmt:trait :trait1;  #colours same as Male
        tmt:trait :trait4 . # total length

##### example of scope restriction...not in original source document
:descriptionFemalesInSummer a tmt:Description ;
         tmt:hasScope tmt:females;
         tmt:hasScope tmt:summer;  ##really need previous description to NOT apply in summer
	 tmt:trait :trait1;
	 tmt:trait :trait4Summer .
:trait1Summer a tmt:CategoricalTrait.

:trait1 a tmt:CategoricalTrait ;  #Categorize traits or just values??
	tmt:hasName pto:colours ;
        tmt:hasValue :value1 .


:trait2 a tmt:QuantitativeTrait ;
     tmt:hasName "Total length";
     tmt:hasScope "with cepalic lobe";
     tmt:hasValue :value2 .

:trait3 a tmt:QuantitativeTrait ;
     tmt:hasName "Total length";
     tmt:hasScope "without cepalic lobe";
     tmt:hasValue :value3 .

:value1 a tmt:CategoricalValue;
     tmt:value "variable" ;
     tmt:modifier sdd:slightly .
	

:value2 a tmt:QuantitativeValue;
   measure:min 1.49 ;
   measure:max 1.66 ;
   measure:mean 1.58 .


:value3 a tmt:QuantitativeValue;  
   measure:min 1.37 ;
   measure:max 1.154 ;
   measure:mean 1.146 .

####### Female length traits
:trait4 a tmt:QuantitativeTrait ;
     tmt:hasName "Total length";
     tmt:hasValue :value4 .

:value4 a tmt:QuantitativeValue;  
   measure:min 1.32 ;
   measure:max 1.157 ;
   measure:mean 1.146 .
