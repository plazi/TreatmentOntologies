###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#First_Revisor_Choosing_Spelling

:First_Revisor_Choosing_Spelling rdf:type owl:ObjectProperty ;
                                 
                                 skos:prefLabel "First Revisor, Choosing Spelling"@en ;
                                 
                                 skos:scopeNote "ICZN"@en ;
                                 
                                 <http://purl.org/dc/elements/1.1/type> "Term" ;
                                 
                                 skos:definition "The first revisor of a zoological name which is spelled in more than one way in the original work selecting the correct spelling for the name, under ICZN Article 24.2.3. 'Selection of correct original spellings. If a name is spelled in more than one way in the original work, the first author to have cited them together and to have selected one spelling as correct is the First Reviser'."@en ;
                                 
                                 rdfs:domain [ rdf:type owl:Restriction ;
                                               owl:onProperty :hasNomenclaturalStatus ;
                                               owl:hasValue :ReplacementOfAName
                                             ] .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Forgotten_Name

:Forgotten_Name rdf:type owl:ObjectProperty ;
                
                rdfs:comment "ICZN Glossary: nomen oblitum: applied after 1 January 2000 to a name, unused since 1899, which as a result of an action taken under Article 23.9.2 does not take precedence over a younger synonym or homonym in prevailing usage; the younger name which takes precedence over the nomen oblitum may be called a nomen protectum (q.v.)."^^xsd:string ;
                
                <http://purl.org/dc/elements/1.1/title> "nomen oblitum"^^xsd:string ;
                
                skos:prefLabel "Forgotten name"@en ;
                
                skos:editorialNote "Should this be a separate individual from nomen oblitum, or an alternate label for nomen oblitum?"@en ;
                
                <http://purl.org/dc/elements/1.1/type> "Term" ;
                
                rdfs:domain [ rdf:type owl:Restriction ;
                              owl:onProperty :hasNomenclaturalStatus ;
                              owl:hasValue :Nomen_Oblitum
                            ] .



