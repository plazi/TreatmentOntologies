###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Senior_Homonym

:Senior_Homonym rdf:type owl:ObjectProperty ;
                
                <http://purl.org/dc/elements/1.1/description> "The member of a pair (or larger set) of identical names for different types that has priority over the other names in the set." ;
                
                skos:definition "The member of a pair (or larger set) of identical names for different types that has priority over the other names in the set." ;
                
                skos:scopeNote "ICNP"@en ,
                               "ICZN"@en ;
                
                <http://purl.org/dc/elements/1.1/title> "Senior Homonym"@en ;
                
                skos:prefLabel "Senior Homonym"@en ;
                
                <http://purl.org/dc/elements/1.1/type> "Term" ;
                
                rdfs:domain [ rdf:type owl:Restriction ;
                              owl:onProperty :hasNomenclaturalStatus ;
                              owl:hasValue :Available_ICZN
                            ] .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Senior_Objective_Synonym

:Senior_Objective_Synonym rdf:type owl:ObjectProperty ;
                          
                          <http://purl.org/dc/elements/1.1/title> "Senior Objective Synonym"@en ;
                          
                          skos:prefLabel "Senior Objective Synonym"@en ;
                          
                          <http://purl.org/dc/elements/1.1/type> "Term" ;
                          
                          <http://purl.org/dc/elements/1.1/description> "The status of being a senior objective synonym under the sense of the ICZN.  The earlier of a pair of nominal taxa that have the same name bearing type." ;
                          
                          skos:scopeNote "ICZN"@en ;
                          
                          skos:definition "The status of being a senior objective synonym in the sense of the ICZN.  The earlier of a pair of nominal taxa that have the same name bearing type." ;
                          
                          skos:scopeNote "ICNP"@en ;
                          
                          rdfs:seeAlso :Junior_Objective_Synonym ;
                          
                          rdfs:domain [ rdf:type owl:Restriction ;
                                        owl:onProperty :hasNomenclaturalStatus ;
                                        owl:hasValue :Available_ICZN
                                      ] .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Senior_Primary_Homonym

:Senior_Primary_Homonym rdf:type owl:ObjectProperty ;
                        
                        <http://purl.org/dc/elements/1.1/description> "The member of a pair (or larger set) of names that were identical when originally formed and are for different types that has priority over the other names in the set." ;
                        
                        skos:scopeNote "ICZN"@en ;
                        
                        skos:prefLabel "Senior Primary Homonym"@en ;
                        
                        <http://purl.org/dc/elements/1.1/title> "Senior Primary Homonym"@en ;
                        
                        <http://purl.org/dc/elements/1.1/type> "Term" ;
                        
                        skos:definition "The member of a pair (or larger set) of identical names for different types that has priority over the other names in the set." ;
                        
                        rdfs:seeAlso :Junior_Primary_Homonym ;
                        
                        rdfs:domain [ rdf:type owl:Restriction ;
                                      owl:onProperty :hasNomenclaturalStatus ;
                                      owl:hasValue :Available_ICZN
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :hasNomenclaturalStatus ;
                                      owl:hasValue :Senior_Homonym
                                    ] .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Senior_Secondary_Homonym

:Senior_Secondary_Homonym rdf:type owl:ObjectProperty ;
                          
                          <http://purl.org/dc/elements/1.1/type> "Term" ;
                          
                          skos:scopeNote "ICZN"@en ;
                          
                          skos:prefLabel "Senior Secondary Homonym"@en ;
                          
                          <http://purl.org/dc/elements/1.1/title> "Senior Secondary Homonym"@en ;
                          
                          skos:definition "The member of a pair (or larger set) of identical zoological or bacterial names for different types that has priority over the other names in the set where the name lacking in priority was not originally in the same combination as the name with priority." ;
                          
                          <http://purl.org/dc/elements/1.1/description> "The member of a pair (or larger set) of zoological or bacterial names that were not identical when originally formed and are for different types that has priority over the other names in the set." ;
                          
                          rdfs:seeAlso :Junior_Secondary_Homonym ;
                          
                          rdfs:domain [ rdf:type owl:Restriction ;
                                        owl:onProperty :hasNomenclaturalStatus ;
                                        owl:hasValue :Senior_Homonym
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :hasNomenclaturalStatus ;
                                        owl:hasValue :Available_ICZN
                                      ] .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Senior_Subjective_Synonym

