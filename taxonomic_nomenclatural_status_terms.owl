###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Absolute_Synonym

:Absolute_Synonym rdf:type owl:ObjectProperty ;
                  
                  <http://purl.org/dc/elements/1.1/description> "A general term for synonyms based upon the same type."^^xsd:string ;
                  
                  skos:definition "A general term for synonyms based upon the same type."^^xsd:string ;
                  
                  <http://purl.org/dc/elements/1.1/description> "Hawksworth: 'A homotypic synonym, a nomenclatural synonym (botany), an objective synonym (zoology). '"^^xsd:string ;
                  
                  rdfs:comment "Implied in calling a name an absolute synonym is that it is a Junior/Later Absolute Synonym"^^xsd:string ;
                  
                  skos:scopeNote "ICNB"@en ,
                                 "ICZN"@en ;
                  
                  <http://purl.org/dc/elements/1.1/type> "Term" ;
                  
                  skos:scopeNote "ICBN"@en ;
                  
                  rdfs:domain [ rdf:type owl:Restriction ;
                                owl:onProperty :hasNomenclaturalStatus ;
                                owl:hasValue :Nomen_Abortivum
                              ] .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Changed_Combination_ICZN

:Changed_Combination_ICZN rdf:type owl:ObjectProperty ;
                          
                          <http://purl.org/dc/elements/1.1/type> "Term" ;
                          
                          skos:editorialNote "Check this, Changed Combination sensu ICZN has both Taxonomic and Nomenclatural event types" ;
                          
                          skos:scopeNote "ICZN"@en ;
                          
                          rdfs:domain [ rdf:type owl:Restriction ;
                                        owl:onProperty :isNomenclaturalEventType ;
                                        owl:hasValue :Change_of_combination
                                      ] .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Doubtfull_Name

:Doubtfull_Name rdf:type owl:ObjectProperty ;
                
                rdfs:comment "ICNP.   A name of doubtfull or uncertain application."^^xsd:string ;
                
                <http://purl.org/dc/elements/1.1/type> "Term" ;
                
                skos:prefLabel "Doubtfull name"@en ;
                
                rdfs:domain [ rdf:type owl:Restriction ;
                              owl:onProperty :hasNomenclaturalStatus ;
                              owl:hasValue :Dubious_Name
                            ] .



###  http://webprojects.huh.harvard.edu/ontologies/taxonomic_nomenclatural_status_terms.owl#Earlier_Nomenclatural_Synonym

:Earlier_Nomenclatural_Synonym rdf:type owl:ObjectProperty ;
                               
                               skos:prefLabel "Earlier Nomenclatural Synonym"@en ;
                               
